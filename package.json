{
  "name": "hotkey-notion",
  "version": "1.0.0",
  "description": "A command line tool enabling you to save and read your favorite hotkeys from a local Notion database.",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rioredwards/hotkey-notion.git"
  },
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --esm src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cli",
    "notion",
    "hotkey",
    "productivity"
  ],
  "author": "Rio Edwards <rioredwards@gmail.com>",
  "homepage": "https://github.com/rioredwards/hotkey-notion#readme",
  "bugs": {
    "url": "https://github.com/rioredwards/hotkey-notion/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@notionhq/client": "^2.2.4",
    "@types/node": "^18.15.11",
    "chalk": "^5.2.0",
    "chalk-animation": "^2.0.3",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.3",
    "nanospinner": "^1.1.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3"
  }
}
